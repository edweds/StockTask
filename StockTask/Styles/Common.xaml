<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:StockTask.WPF.Styles">
    <!--Basic colors-->
    <Color x:Key="BaseColor1">#566573</Color>
    <Color x:Key="BaseColor2">#DAF7A6</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BasicBrush1" Color="{StaticResource BaseColor1}"/>
    <SolidColorBrush x:Key="BasicBrush2" Color="{StaticResource BaseColor2}"/>

</ResourceDictionary>